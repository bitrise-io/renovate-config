{
  "extends": [
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    ":autodetectPinVersions",
    ":prConcurrentLimit20",
    "group:monorepos",
    "group:recommended",
    "workarounds:all"
  ],
  "labels": ["renovate", "renovate-swat"],
  "dependencyDashboard": false,
  "rebaseWhen": "never",
  "transitiveRemediation": true,
  "packageRules": [{
    "matchPackagePatterns": ["*"],
    "enabled": false
  }],
  "vulnerabilityAlerts": {
    "automerge": false,
    "enabled": true,
    "labels": ["security"]
  }
}
